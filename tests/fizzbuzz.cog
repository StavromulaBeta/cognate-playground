Def Fizzbuzz (
	Let N;
	Def Multiple as ( Zero? Modulo Swap N );
	If Multiple of 15 then "fizzbuzz"
	If Multiple of 3  then "fizz"
	If Multiple of 5  then "buzz"
		else N
);


Let L be Map (Fizzbuzz) over Range 1 to 100;

Print If == "fizz" Index 5 of L
	then "PASS: Fizzbuzz 1"
	else "FAIL: Fizzbuzz 1";

Print If == "fizzbuzz" Index 14 of L
	then "PASS: Fizzbuzz 2"
	else "FAIL: Fizzbuzz 2";

Print If == 16 Index 15 of L
	then "PASS: Fizzbuzz 3"
	else "FAIL: Fizzbuzz 3";
